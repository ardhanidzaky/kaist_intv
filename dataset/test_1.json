{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data": {"url": "data.csv"},
    "mark": "bar",
    "encoding": {
        "x": {
            "field": "fuel",
            "type": "nominal"
        },
        "y": {
            "field": "selling_price",
            "aggregate": "mean",
            "type": "quantitative"
        }
    }
}