{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data": {"url": "data.csv"},
    "mark": "bar",
    "encoding": {
        "x": {
            "field": "engine_displacement",
            "bin": true
        },
        "y": {
            "aggregate": "count"
        },
        "color": {"field": "fuel_type"}
    }
}